package filters.common;

import filters.common.constants.*;

actor sub() int(size=SAMPLE_SZ) operand_1 , int(size=SAMPLE_SZ) operand_2 ==> int(size=SAMPLE_SZ) result :

	action operand_1:[ x ], operand_2:[ y ] ==> result:[ x - y ]
	end

end
